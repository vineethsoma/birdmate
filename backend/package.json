{
  "name": "birdmate-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite-node --watch src/server.ts",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "migrate": "node --loader ts-node/esm src/db/migrations/run.ts",
    "seed": "npm run download:taxonomy && npm run seed:taxonomy && npm run fetch:images && npm run generate:embeddings",
    "download:taxonomy": "vite-node src/db/seeds/downloadTaxonomy.ts",
    "seed:taxonomy": "vite-node src/db/seeds/seedTaxonomy.ts",
    "fetch:images": "vite-node src/db/seeds/fetchImages.ts",
    "generate:embeddings": "vite-node src/db/seeds/generateEmbeddings.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "openai": "^4.73.0",
    "better-sqlite3": "^11.7.0",
    "sqlite-vss": "^0.1.2",
    "validator": "^13.11.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/better-sqlite3": "^7.6.8",
    "@types/validator": "^13.11.7",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-node": "^1.1.0",
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "prettier": "^3.1.1"
  }
}
